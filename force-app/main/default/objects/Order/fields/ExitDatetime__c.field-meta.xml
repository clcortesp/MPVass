<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>ExitDatetime__c</fullName>
    <externalId>false</externalId>
    <formula>IF(  ISPICKVAL($User.TimeZoneSidKey , &quot;America/Punta_Arenas&quot;),
